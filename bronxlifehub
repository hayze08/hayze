-- bl hub by hayze dont steal 

-- Load Rayfield Library
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Create the window with no key and the title "bronxlife hub by hayze"
local Window = Rayfield:CreateWindow({
    Name = "bronxlife hub by hayze",
    LoadingTitle = "Loading Hub...",
    LoadingSubtitle = "Please Wait",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = nil,
        FileName = "RayfieldConfig"
    },
    Discord = {
        Enabled = false,
        Invite = "",
        RememberJoins = false
    },
    KeySystem = false,
    KeySettings = {
        Title = "Key System",
        Subtitle = "Enter the key to continue",
        Note = "No key required"
    }
})

-- Create 'ESP' category
local ESPCategory = Window:CreateTab("ESP", 4483362458) -- You can replace with your own icon ID

-- Add the "Check opps for mpr" button under 'ESP'
ESPCategory:CreateButton({
    Name = "Check opps for mpr",
    Callback = function()
        loadstring(game:HttpGet("https://pastebin.com/raw/1AmRHSVd"))()
    end
})

-- Create 'AimBot' category
local AimBotCategory = Window:CreateTab("AimBot", 4483362458) -- Replace with custom icon if desired

-- Add the "Enable AimBot" button under 'AimBot'
AimBotCategory:CreateButton({
    Name = "Enable AimBot",
    Callback = function()
        loadstring(game:HttpGet("https://pastebin.com/raw/uvDhQZ6q"))()
    end
})

-- Create 'Teleport' category
local TeleportCategory = Window:CreateTab("Teleport", 4483362458) -- Replace with your own icon ID

-- Add the "TP Mpr turf" toggle button under 'Teleport'
local toggleState = false -- Track toggle state (enabled/disabled)
local teleportScript -- Variable to store the teleportation script

-- Function to execute the teleportation script
local function enableTeleport()
    teleportScript = loadstring(game:HttpGet("https://pastebin.com/raw/MnWT8ffi"))()

-- Function to disable the teleportation script
local function disableTeleport()
    if teleportScript then
        -- Since the script from pastebin doesn't have a direct way to "stop", we can attempt to disconnect events or reset things here
        -- For now, we will just clear the variable as a placeholder for stopping logic
        teleportScript = nil
    end
end

-- Add toggle to enable/disable teleportation
TeleportCategory:CreateToggle({
    Name = "TP Mpr turf",
    CurrentValue = toggleState,
    Callback = function(State)
        toggleState = State
        if toggleState then
            -- Enable teleportation
            enableTeleport()
        else
            -- Disable teleportation
            disableTeleport()
        end
    end
})

-- Display the window
Rayfield:Init()
